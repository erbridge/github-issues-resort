{
	"name": "GitHub Issues Resort",
	"version": "1.0.2",
	"description": "Show Issues sorted by last update by default.",
	"homepage_url": "https://github.com/erbridge/github-issues-resort",
	"manifest_version": 2,
	"minimum_chrome_version": "36",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"https://github.com/*/*/issues"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	}
}
